"amixer set Master 3dB+ unmute"
	XF86AudioRaiseVolume

"amixer set Master 3dB- unmute"
	XF86AudioLowerVolume

"amixer set Master toggle"
	XF86AudioMute

"import -window root $HOME/Desktop/screenshot-`date +%F-%H%M%S`.png"
	m:0x0 + c:107
	Print

"import $HOME/Desktop/screenshot-`date +%F-%H%M%S`.png"
	m:0x1 + c:107
	Shift + Print

"xbacklight -dec 10%"
	XF86MonBrightnessDown

"xbacklight -inc 10%"
	XF86MonBrightnessUp

