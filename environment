SNY=/n/sny/am
UIO=/n/uio
ICLOUD="$HOME/Library/Mobile Documents/com~apple~CloudDocs"

XDG_CACHE_HOME=$HOME/tmp
XDG_CONFIG_HOME=$HOME/rc
XDG_DATA_HOME=$HOME/lib

BASH_ENV=$XDG_CONFIG_HOME/bash/bashrc
BASH_SILENCE_DEPRECATION_WARNING=1
GNUPGHOME=$XDG_DATA_HOME/gnupg
HISTCONTROL=ignoreboth:erasedups
HISTFILE=$XDG_CACHE_HOME/bash/history
INPUTRC=$XDG_CONFIG_HOME/readline/inputrc
LESSHISTFILE=/dev/null

PLAN9=/usr/local/plan9
GOPATH=$SNY
GOBIN=$SNY/bin
PYTHONDONTWRITEBYTECODE=1
JAVA_HOME=/lib/jvm/java

TMPDIR=${TMPDIR:-/tmp/}
NAMESPACE=${NAMESPACE:-${TMPDIR}ns.${USER}.default}
EDITOR=E

PATH=$HOME/bin:/usr/local/bin:$PATH:$PLAN9/bin

LC_ALL=en_US.UTF-8
