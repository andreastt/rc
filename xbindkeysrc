"amixer set Master 5%+ unmute"
	XF86AudioRaiseVolume

"amixer set Master 5%- unmute"
	XF86AudioLowerVolume

"amixer set Master toggle"
	XF86AudioMute

"gnome-screenshot -f $HOME/Desktop/screenshot-`date +%F-%H%M%S`.png"
	m:0x0 + c:107
	Print

"gnome-screenshot -a -f $HOME/Desktop/screenshot-`date +%F-%H%M%S`.png"
	m:0x1 + c:107
	Shift + Print

"xbacklight -dec 10%"
	XF86MonBrightnessDown

"xbacklight -inc 10%"
	XF86MonBrightnessUp

