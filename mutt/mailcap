text/html; /usr/bin/w3m -dump -s -o display_link=yes -o display_link_number=yes -o decode_url=yes -T text/html -I %{charset}; copiousoutput; description=HTML Text

application/pdf; mutt_bgrun xdg-open 2>/dev/null %s; test=test -n "$DISPLAY"; nametemplate=%s.pdf; description="PDF Document"
application/pdf; pdftotext -htmlmeta %s - | w3m -dump -T text/html; nametemplate=%s.pdf; copiousoutput
application/x-pdf; mutt_bgrun xdg-open 2>/dev/null %s; test=test -n "$DISPLAY"; nametemplate=%s.pdf; description="PDF Document"
application/x-pdf; pdftotext -htmlmeta %s - | w3m -dump -T text/html; nametemplate=%s.pdf; copiousoutput
